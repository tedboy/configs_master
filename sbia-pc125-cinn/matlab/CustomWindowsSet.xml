<?xml version="1.0" encoding="utf-8"?>
<CustomKeySet derivedfrom="Windows" modifieddefault="false">
   <Context id="Global">
      <Action id="dock">
         <Stroke code="VK_D" ctrl="on" shift="on"/>
         <Stroke code="VK_F8"/>
      </Action>
      <Action id="undock">
         <Stroke code="VK_U" ctrl="on" shift="on"/>
         <Stroke code="VK_F7"/>
      </Action>
      <Action id="show-preferences">
         <Stroke alt="on" code="VK_3"/>
         <Stroke code="VK_3" meta="on"/>
      </Action>
      <Action id="split-screen-top-bottom">
         <Stroke alt="on" code="VK_2"/>
         <Stroke code="VK_2" meta="on"/>
      </Action>
      <Action id="save-as">
         <Stroke code="VK_S" ctrl="on" shift="on"/>
      </Action>
      <Action id="inc-search-forward"/>
      <Action id="split-screen-off">
         <Stroke alt="on" code="VK_3"/>
         <Stroke code="VK_3" meta="on"/>
      </Action>
      <Action id="split-screen-left-right">
         <Stroke alt="on" code="VK_1"/>
         <Stroke code="VK_1" meta="on"/>
      </Action>
   </Context>
</CustomKeySet>