<?xml version="1.0" encoding="utf-8"?>
<CustomKeySet derivedfrom="Windows" modifieddefault="false">
   <Context id="Global">
      <Action id="caret-previous-word">
         <Stroke code="VK_LEFT" ctrl="on"/>
         <Stroke code="VK_KP_LEFT" ctrl="on"/>
         <Stroke code="VK_B" ctrl="on" shift="on"/>
         <Stroke alt="on" code="VK_B" shift="on"/>
         <Stroke code="VK_B" meta="on" shift="on"/>
      </Action>
      <Action id="print-image"/>
      <Action id="remove-word-next">
         <Stroke code="VK_DELETE" ctrl="on"/>
         <Stroke code="VK_L" ctrl="on" shift="on"/>
      </Action>
      <Action id="kill-line">
         <Stroke code="VK_K" ctrl="on"/>
      </Action>
      <Action id="split-screen-top-bottom">
         <Stroke code="VK_3" ctrl="on"/>
      </Action>
      <Action id="new-mfile">
         <Stroke code="VK_N" ctrl="on"/>
      </Action>
      <Action id="caret-backward">
         <Stroke code="VK_LEFT"/>
         <Stroke code="VK_KP_LEFT"/>
         <Stroke code="VK_B" ctrl="on"/>
      </Action>
      <Action id="find-files"/>
      <Action id="split-screen-left-right">
         <Stroke code="VK_2" ctrl="on"/>
      </Action>
      <Action id="next-cell">
         <Stroke code="VK_DOWN" ctrl="on"/>
         <Stroke code="VK_KP_DOWN" ctrl="on"/>
         <Stroke code="VK_N" ctrl="on" shift="on"/>
      </Action>
      <Action id="find-and-replace">
         <Stroke code="VK_H" ctrl="on"/>
      </Action>
      <Action id="select-all">
         <Stroke code="VK_A" ctrl="on" shift="on"/>
      </Action>
      <Action id="prev-cell">
         <Stroke code="VK_UP" ctrl="on"/>
         <Stroke code="VK_KP_UP" ctrl="on"/>
         <Stroke code="VK_P" ctrl="on" shift="on"/>
      </Action>
      <Action id="caret-down">
         <Stroke code="VK_DOWN"/>
         <Stroke code="VK_KP_DOWN"/>
         <Stroke code="VK_N" ctrl="on"/>
      </Action>
      <Action id="caret-begin-line">
         <Stroke code="VK_HOME"/>
         <Stroke code="VK_A" ctrl="on"/>
      </Action>
      <Action id="caret-end-line">
         <Stroke code="VK_END"/>
         <Stroke code="VK_E" ctrl="on" shift="on"/>
         <Stroke code="VK_E" ctrl="on"/>
      </Action>
      <Action id="caret-up">
         <Stroke code="VK_UP"/>
         <Stroke code="VK_KP_UP"/>
         <Stroke code="VK_P" ctrl="on"/>
      </Action>
      <Action id="print"/>
      <Action id="caret-next-word">
         <Stroke code="VK_RIGHT" ctrl="on"/>
         <Stroke code="VK_KP_RIGHT" ctrl="on"/>
         <Stroke code="VK_F" ctrl="on" shift="on"/>
         <Stroke alt="on" code="VK_F" shift="on"/>
         <Stroke code="VK_F" meta="on" shift="on"/>
      </Action>
      <Action id="split-screen-off">
         <Stroke code="VK_1" ctrl="on"/>
      </Action>
      <Action id="caret-forward">
         <Stroke code="VK_RIGHT"/>
         <Stroke code="VK_KP_RIGHT"/>
         <Stroke code="VK_F" ctrl="on"/>
      </Action>
   </Context>
   <Context id="SpreadsheetTable"/>
   <Context id="Recordlist"/>
   <Context id="MATLABEditor">
      <Action id="delete-next">
         <Stroke code="VK_L" ctrl="on"/>
      </Action>
      <Action follows="None" id="new-mfile"/>
   </Context>
   <Context id="CommandWindow">
      <Action id="kill-line"/>
      <Action id="caret-end-line"/>
      <Action id="delete-next">
         <Stroke code="VK_L" ctrl="on"/>
      </Action>
      <Action id="caret-backward"/>
   </Context>
</CustomKeySet>